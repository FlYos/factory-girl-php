{
    "name": "flyos/factory-girl-php",
    "type": "library",
    "description": "Fixture replacement for focused and readable tests - A PHP port of Thoughtbot's Ruby Factory Girl",
    "keywords": [
        "factory girl",
        "fixture factory",
        "fixture replacement",
        "object mother",
        "testing",
        "fixture",
        "tdd"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Steven Titren",
        "email": "contact@webaki.com"
    }],
    "require": {
        "php": ">=5.6.0",
        "doctrine/orm": "^2.5",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-0": {
            "FactoryGirl": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "FactoryGirl\\Tests": "tests",
            "Doctrine\\Tests": "vendor/doctrine/dbal/tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    }
}
